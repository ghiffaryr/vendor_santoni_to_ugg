ui_print("                Ugg Vendor Patch                ");
ui_print("         by ghiffaryr @ xda-developers          ");
ui_print("  for porting santoni non-treble based rom only ");
mount("ext4", "/dev/block/bootdevice/by-name/system", "/system");
package_extract_dir("vendor", "/system/system/vendor");
delete("/system/system/vendor/lib/hw/fingerprint.msm8937.so");
set_perm(0, 0, 0755, "/system/system/vendor/bin/fingerprintd");
set_perm(0, 0, 0755, "/system/system/vendor/bin/goodixfingerprintd");
ui_print("                                                ");
ui_print("Done!");